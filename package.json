{
  "name": "tablecheck-frontend",
  "description": "TableCheck Inc. Frontend settings and scripts",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "repository": {
    "type": "git",
    "url": "git@github.com:tablecheck/frontend.git"
  },
  "version": "0.0.0",
  "scripts": {
    "audit": "tablecheck-frontend-audit && nx run-many --parallel=false --target=audit",
    "audit:ci": "npx tablecheck-frontend-audit --ci && nx run-many --target=audit:ci",
    "lint": "nx affected --target=lint && prettier -c .",
    "format": "nx affected --target=lint:format && prettier -w .",
    "test": "nx affected --target=test",
    "test:watch": "nx run-many --target=test:watch",
    "start": "start-storybook -p 6006",
    "build": "nx affected --target=build",
    "build:docs": "build-storybook",
    "prepare": "husky install && npm run build",
    "all-upgrade": "npm-upgrade && lerna exec --concurrency 1 -- npm-upgrade"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@auto-it/all-contributors": "10.46.0",
    "@auto-it/first-time-contributor": "10.46.0",
    "@auto-it/npm": "10.46.0",
    "@auto-it/released": "10.46.0",
    "@auto-it/slack": "10.46.0",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-lerna-scopes": "17.6.6",
    "@emotion/babel-plugin": "^11.11.0",
    "@nx/eslint-plugin": "16.5.0",
    "@nx/jest": "16.5.0",
    "@nx/js": "16.5.0",
    "@nx/linter": "16.5.0",
    "@nx/vite": "16.5.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/node-logger": "6.5.16",
    "@storybook/react": "6.5.16",
    "@swc-node/register": "~1.6.6",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.68",
    "@tablecheck/eslint-config": "file:packages/eslint-config",
    "@tablecheck/frontend-utils": "file:packages/frontend-utils",
    "@tablecheck/prettier-config": "file:packages/prettier-config",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "18.7.1",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "auditjs": "4.0.40",
    "auto": "10.46.0",
    "eslint": "^8",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "husky": "8.0.3",
    "jsdom": "~20.0.3",
    "jsonc-eslint-parser": "^2.3.0",
    "lerna": "7.1.1",
    "lint-staged": "^13.2.3",
    "lodash": "^4.17.21",
    "nx": "16.5.0",
    "nx-cloud": "16.0.5",
    "prettier": "^3.0.0",
    "storybook": "^6.5.16",
    "type-fest": "3.12.0",
    "typescript": "5.1.6",
    "vite": "^4.4.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@nx/devkit": "16.5.0",
    "@nx/plugin": "16.5.0",
    "@swc/helpers": "~0.5.0",
    "tslib": "^2.6.0"
  }
}
