{
  "name": "@tablecheck/frontend-quality",
  "description": "Typescript, eslint and assorted linters/code quality checks",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "tablecheck-frontend-precommit": "bin/precommit.js",
    "tablecheck-frontend-quality": "bin/lint.js"
  },
  "files": [
    "bin",
    "lib",
    "templates"
  ],
  "scripts": {
    "build": "node ../../scripts/deriveExportsFromSiblings.mjs && npm run build:main && npm run build:formatters && node ./scripts/fixFormatters.js",
    "build:main": "cp main.build.config.ts build.config.ts && unbuild || rm build.config.ts",
    "build:formatters": "cp formatters.build.config.ts build.config.ts && unbuild || rm build.config.ts",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.20.13",
    "@tablecheck/babel-preset": "^1.1.0",
    "@tablecheck/eslint-config": "^1.8.2",
    "@tablecheck/frontend-typescript": "^0.0.0",
    "@tablecheck/frontend-utils": "^1.1.0",
    "@typescript-eslint/parser": "^5.52.0",
    "ansi-regex": "^6.0.1",
    "chalk": "^5.2.0",
    "eslint": "^8",
    "execa": "^7.0.0",
    "find-git-root": "^1.0.4",
    "flat": "^5.0.2",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "jscodeshift": "^0.14.0",
    "lint-staged": "^13.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "prettier-package-json": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "semver": "^7.3.8",
    "staged-git-files": "^1.3.0",
    "strip-ansi": "^7.0.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/eslint": "8.21.1",
    "@types/flat": "5.0.2",
    "@types/semver": "7.3.13",
    "@types/text-table": "0.2.2",
    "type-fest": "3.5.7",
    "typescript": "4.9.5",
    "unbuild": "^1.1.1"
  },
  "engines": {
    "node": ">= 16.16.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
