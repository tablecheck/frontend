{
  "name": "tablecheck-frontend",
  "description": "TableCheck Inc. react settings and scripts",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "repository": {
    "type": "git",
    "url": "git@github.com:tablecheck/frontend.git"
  },
  "version": "0.0.0",
  "scripts": {
    "audit": "packages/audit/bin/main.js",
    "audit:ci": "packages/audit/bin/main.js --ci",
    "format": "packages/quality/bin/main.js --fix",
    "lint": "packages/quality/bin/main.js",
    "test": "vitest",
    "start": "start-storybook -p 6006",
    "build": "lerna run build",
    "rebuild": "rm -rf packages/**/lib && lerna run build",
    "build:docs": "build-storybook",
    "prepare": "husky install && lerna bootstrap --ignore-prepublish && npm run build",
    "precommit": "packages/quality/bin/main.js --precommit",
    "all-upgrade": "npm-upgrade && lerna exec --concurrency 1 -- npm-upgrade"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "10.42.0",
    "@auto-it/first-time-contributor": "10.42.0",
    "@auto-it/npm": "10.42.0",
    "@auto-it/released": "10.42.0",
    "@auto-it/slack": "10.42.0",
    "@commitlint/cli": "17.4.3",
    "@commitlint/config-lerna-scopes": "17.4.2",
    "@emotion/babel-plugin": "^11.10.5",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/node-logger": "6.5.16",
    "@storybook/react": "6.5.16",
    "@tablecheck/eslint-config": "^1.8.2",
    "@types/fs-extra": "^11.0.1",
    "@vitejs/plugin-react": "3.1.0",
    "auditjs": "4.0.39",
    "auto": "10.42.0",
    "eslint": "^8",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "husky": "8.0.3",
    "lerna": "6.5.1",
    "lint-staged": "^13.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "storybook": "^6.5.16",
    "typescript": "4.9.5",
    "vite": "4",
    "vitest": "0.28.5"
  },
  "dependencies": {
    "unbuild": "1.1.1"
  }
}
