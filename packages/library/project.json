{
  "name": "library",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/library/src",
  "projectType": "library",
  "targets": {
    "audit": {
      "executor": "@tablecheck/nx:audit",
      "outputs": ["{options.outputFile}"]
    },
    "lint": {
      "executor": "@tablecheck/nx:quality",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/library/**/*.ts"]
      }
    },
    "lint:format": {
      "executor": "@tablecheck/nx:quality",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/library/**/*.ts"],
        "fix": true
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/packages/library"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/library"
      }
    }
  },
  "tags": []
}
