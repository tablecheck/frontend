{
  "extends": "../../tsconfig.base.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/*.ts",
    "src/*.tsx",
    "src/**/*.json",
    "src/*.json"
  ],
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "paths": {},
    "types": ["vitest"],
    "moduleResolution": "Node",
    "resolveJsonModule": false,
    "target": "ES2020",
    "module": "commonjs"
  },
  "exclude": ["node_modules"],
  "files": []
}
