{
  "extends": "../typescript/tsconfig/base.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/*.ts",
    "src/*.tsx",
    "src/**/*.json",
    "src/*.json",
    "*.ts",
    ".storybook/**/*.ts",
    ".storybook/**/*.tsx",
    ".storybook/*.ts",
    ".storybook/*.tsx",
    ".storybook/**/*.json",
    ".storybook/*.json"
  ],
  "compilerOptions": { "rootDir": ".", "baseUrl": "./src", "composite": false },
  "exclude": ["node_modules"],
  "files": []
}
