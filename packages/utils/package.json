{
  "name": "@tablecheck/frontend-utils",
  "description": "Generic utils shared inside @tablecheck packages",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "version": "2.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ../../scripts/deriveExportsFromSiblings.mjs && tsc -b",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "cosmiconfig": "^8.2.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.1",
    "jiti": "^1.18.2",
    "minimist": "^1.2.8",
    "prettier": "^3.0.0",
    "prettier-package-json": "2.8.0"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/prettier": "2.7.3",
    "@types/prettier-package-json": "2.3.0",
    "@types/semver": "7.5.0",
    "@types/string-similarity": "4.0.0",
    "type-fest": "3.12.0",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">= 16.16.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "config": {
      "optional": true
    }
  }
}
