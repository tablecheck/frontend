{
  "name": "@tablecheck/frontend-typescript",
  "description": "Typescript configuration setup",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "tablecheck-scripts-postinstall": "bin/main.js",
    "tablecheck-tsc-setup": "bin/main.js"
  },
  "files": [
    "bin",
    "lib",
    "tsconfig"
  ],
  "scripts": {
    "build": "node ../../scripts/deriveExportsFromSiblings.mjs && unbuild",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@tablecheck/frontend-utils": "^1.1.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "execa": "^7.0.0",
    "find-git-root": "^1.0.4",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0"
  },
  "peerDependencies": {
    "typescript": "4.x"
  },
  "devDependencies": {
    "type-fest": "3.5.7",
    "typescript": "4.8.2",
    "unbuild": "^1.1.1"
  },
  "engines": {
    "node": ">= 16.16.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts"
}
