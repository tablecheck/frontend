{
  "name": "@tablecheck/frontend-utils",
  "description": "Generic utils shared inside @tablecheck packages",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "version": "1.1.0",
  "type": "module",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "node ../../scripts/deriveExportsFromSiblings.mjs && unbuild",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cosmiconfig": "^8.0.0",
    "execa": "^7.0.0",
    "fs-extra": "^11.1.0",
    "jiti": "^1.17.0",
    "minimist": "^1.2.8",
    "prettier": "^2.8.4",
    "string-similarity": "^4.0.4"
  },
  "peerDependencies": {
    "config": "3",
    "vite": "3"
  },
  "devDependencies": {
    "@types/prettier": "2.7.2",
    "@types/prettier-package-json": "2.3.0",
    "@types/semver": "7.3.13",
    "@types/string-similarity": "4.0.0",
    "type-fest": "3.5.7",
    "typescript": "4.9.5",
    "unbuild": "1.1.1",
    "vite": "4"
  },
  "engines": {
    "node": ">= 16.16.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./lib/index.d.ts",
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "config": {
      "optional": true
    }
  }
}
