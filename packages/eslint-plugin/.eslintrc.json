{
  "ignorePatterns": ["!**/*"],
  "parserOptions": {
    "sourceType": "script"
  },
  "extends": ["../../.eslintrc.js", "plugin:eslint-plugin/recommended"],
  "rules": {
    "eslint-plugin/require-meta-docs-url": "error",
    "eslint-plugin/require-meta-docs-description": "error",
    "eslint-plugin/require-meta-schema": "error"
  }
}
