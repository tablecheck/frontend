{
  "name": "tablecheck-react-system",
  "description": "TableCheck Inc. react settings and scripts",
  "license": "MIT",
  "author": "TableCheck Inc.",
  "repository": {
    "type": "git",
    "url": "git@github.com:tablecheck/tablecheck-react-system.git"
  },
  "version": "0.0.0",
  "scripts": {
    "audit": "./packages/audit/lib/main.js",
    "audit:ci": "./packages/audit/lib/main.js --ci",
    "format": "./packages/quality/lib/bin/main.js --fix",
    "lint": "./packages/quality/lib/bin/main.js",
    "test": "vitest",
    "start": "start-storybook -p 6006",
    "build": "lerna run build",
    "build:docs": "build-storybook",
    "prepare": "husky install",
    "postinstall": "lerna bootstrap",
    "precommit": "./packages/quality/lib/main.js --precommit",
    "all-upgrade": "npm-upgrade && lerna exec --concurrency 1 -- npm-upgrade"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "10.37.4",
    "@auto-it/first-time-contributor": "10.37.4",
    "@auto-it/npm": "10.37.4",
    "@auto-it/released": "10.37.4",
    "@auto-it/slack": "10.37.4",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-lerna-scopes": "17.0.2",
    "@emotion/babel-plugin": "^11.10.0",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-docs": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/node-logger": "6.5.10",
    "@storybook/react": "6.5.10",
    "@types/fs-extra": "9.0.13",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "@vitejs/plugin-react": "2.0.1",
    "auditjs": "4.0.37",
    "auto": "10.37.4",
    "babel-preset-razzle": "4.2.17",
    "config": "^3.3.7",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "husky": "8.0.1",
    "lerna": "5.4.3",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "storybook": "^6.5.10",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vitest": "0.22.1"
  }
