{
  "extends": "../../tsconfig.base.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.cts",
    "src/**/*.tsx",
    "src/*.ts",
    "src/*.tsx",
    "src/**/*.json",
    "src/*.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "module": "NodeNext",
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "src/generators/*/files/*",
    "src/generators/*/files/**/*"
  ],
  "files": []
}
